{
  "name": "strapi-keycloak-sso",
  "version": "0.1.2",
  "description": "Keycloak authentication provider for the Strapi v5 administration panel.",
  "license": "MIT",
  "author": "Stanislav Lyu <stas-lyu@outlook.com>",
  "keywords": [
    "strapi",
    "plugin",
    "authentication",
    "keycloak",
    "passport",
    "oidc",
    "admin",
    "v5"
  ],
  "type": "commonjs",
  "main": "dist/server/index.js",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "source": "./admin/src/index.js",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "source": "./server/src/index.js",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "strapi-admin.js",
    "strapi-server.js"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.18",
    "@strapi/icons": "^2.0.0-rc.18",
    "@strapi/helper-plugin": "^4.25.12",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.11.3",
    "axios": "^1.8.2",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "styled-components": "^6.1.16"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.11.3",
    "@strapi/sdk-plugin": "^5.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "styled-components": "^6.1.16"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "strapi": {
    "kind": "plugin",
    "name": "strapi-keycloak-sso",
    "displayName": "Strapi Keycloak SSO",
    "description": "Keycloak authentication provider for administration panel"
  }
}
